示例
--

已复制

1.  import { picker } from '@kit.CoreFileKit';
2.  import { BusinessError } from '@kit.BasicServicesKit';
3.  import { DownloadFileButton, DownloadLayoutDirection } from '@kit.ArkUI';

5.  @Entry
6.  @Component
7.  struct Index {
8.    build() {
9.      Column() {
10.        DownloadFileButton({
11.          contentOptions: {
12.            // icon: DownloadIconStyle.FULL\_FILLED,
13.            // text: DownloadDescription.DOWNLOAD
14.          },
15.          styleOptions: {
16.            iconSize: '16vp',
17.            layoutDirection: DownloadLayoutDirection.HORIZONTAL,
18.            fontSize: '16vp',
19.            fontStyle: FontStyle.Normal,
20.            fontWeight: FontWeight.Medium,
21.            fontFamily: 'HarmonyOS Sans',
22.            fontColor: '#ffffffff',
23.            iconColor: '#ffffffff',
24.            textIconSpace: '4vp'
25.          }
26.        })
27.          .backgroundColor('#007dff')
28.          .borderStyle(BorderStyle.Dotted)
29.          .borderWidth(0)
30.          .borderColor('')
31.          .borderRadius('24vp')
32.          .position({ x: 0, y: 0 })
33.          .markAnchor({ x: 0, y: 0 })
34.          .offset({ x: 0, y: 0 })
35.          .constraintSize({})
36.          .padding({
37.            top: '12vp',
38.            bottom: '12vp',
39.            left: '24vp',
40.            right: '24vp'
41.          })
42.          .onClick(() =\> {
43.            this.downloadAction();
44.          })
45.      }
46.    }

48.    downloadAction() {
49.      try {
50.        const document = new picker.DocumentSaveOptions();
51.        document.pickerMode = picker.DocumentPickerMode.DOWNLOAD;
52.        new picker.DocumentViewPicker().save(document, (err: BusinessError, result: Array<string\>) =\> {
53.          if (err) {
54.            return;
55.          }
56.          console.info(\`downloadAction result:  ${JSON.stringify(result)}\`);
57.        });
58.      } catch (e) {
59.      }
