--

已复制

1.  import { ComposeTitleBar, promptAction } from '@kit.ArkUI'

3.  interface menuItem {
4.    value: Resource;
5.    isEnabled?: boolean;
6.    action?: () =\> void
7.  }

9.  @Entry
10.  @Component
11.  struct Index {
12.    private menuItems: Array<menuItem\> = \[
13.      {
14.        value: $r('app.media.ic\_public\_save'),
15.        isEnabled: true,
16.        action: () =\> promptAction.showToast({ message: "show toast index 1" })
17.      },
18.      {
19.        value: $r('app.media.ic\_public\_save'),
20.        isEnabled: true,
21.        action: () =\> promptAction.showToast({ message: "show toast index 1" })
22.      },
23.      {
24.        value: $r('app.media.ic\_public\_save'),
25.        isEnabled: true,
26.        action: () =\> promptAction.showToast({ message: "show toast index 1" })
27.      },
28.      {
29.        value: $r('app.media.ic\_public\_save'),
30.        isEnabled: true,
31.        action: () =\> promptAction.showToast({ message: "show toast index 1" })
32.      },
33.    \]

35.    build() {
36.      Row() {
37.        Column() {
38.          Divider().height(2).color(0xCCCCCC)
39.          ComposeTitleBar({
40.            title: "标题",
41.            subtitle: "副标题",
42.            menuItems: this.menuItems.slice(0, 1),
43.          })
44.          Divider().height(2).color(0xCCCCCC)
45.          ComposeTitleBar({
46.            title: "标题",
47.            subtitle: "副标题",
48.            menuItems: this.menuItems.slice(0, 2),
49.          })
50.          Divider().height(2).color(0xCCCCCC)
51.          ComposeTitleBar({
52.            title: "标题",
53.            subtitle: "副标题",
54.            menuItems: this.menuItems,
55.          })
56.          Divider().height(2).color(0xCCCCCC)
57.          ComposeTitleBar({
58.            menuItems: \[{ isEnabled: true, value: $r('app.media.ic\_public\_save'),
59.              action: () =\> promptAction.showToast({ message: "show toast index 1" })
60.            }\],
61.            title: "标题",
62.            subtitle: "副标题",
63.            item: { isEnabled: true, value: $r('app.media.app\_icon') }
64.          })
65.          Divider().height(2).color(0xCCCCCC)
66.        }
67.      }.height('100%')
68.    }
69.  }

