25.        state:ItemState.DISABLE
26.      })
27.      this.toolbarList.push({
28.        content: '粘贴',
29.        icon: $r('sys.media.ohos\_ic\_public\_paste'),
30.        action: () => {
31.        },
32.        state:ItemState.ACTIVATE
33.      })
34.      this.toolbarList.push({
35.        content: '全选',
36.        icon: $r('sys.media.ohos\_ic\_public\_select\_all'),
37.        action: () => {
38.        },
39.      })
40.      this.toolbarList.push({
41.        content: '分享',
42.        icon: $r('sys.media.ohos\_ic\_public\_share'),
43.        action: () => {
44.        },
45.      })
46.      this.toolbarList.push({
47.        content: '分享',
48.        icon: $r('sys.media.ohos\_ic\_public\_share'),
