{
    "description": "文本下载按钮，可显示具体下载进度。",
    "interfaces": [
        {
            "description": "ProgressButton({progress: number, content: string, progressButtonWidth?: Length, clickCallback: () => void, enable: boolean})",
            "params": {
                "progress": {
                    "type": "number",
                    "required": true,
                    "description": "下载按钮的当前进度值。"
                },
                "content": {
                    "type": "string",
                    "required": true,
                    "description": "下载按钮的文本。"
                },
                "progressButtonWidth": {
                    "type": ["Length"],
                    "required": false,
                    "description": "下载按钮的宽度。",
                    "default": "44"
                },
                "clickCallback": {
                    "type": "() => void",
                    "required": true,
                    "description": "下载按钮的点击回调。"
                },
                "enable": {
                    "type": "boolean",
                    "required": true,
                    "description": "下载按钮是否可以点击。enable为true时，表示可以点击。enable为false时，表示不可点击。"
                }
            }
        }
    ],
    "attributes": {},
    "events": {},
    "is_common_attrs": true
}